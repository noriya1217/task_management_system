<div class='row field'>
  <div class='col-md-12'>
    <% if @admin.errors.any? %>
      <div id='error_explanation'>
        <h2><%= pluralize(@admin.errors.count, 'error') %></h2>
        <ul>
          <% @admin.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
        </ul>
      </div>
    <% end %>

    <table border=1>
      <tr>
        <th>id</th>
        <td>name</td>
        <td>email</td>
        <td>created_at</td>
        <td>updated_at</td>
      </tr>
      <%= form_with(url: new_admin_user_path, method: :post, local: true) do |form| %>
        <% @users.each do |users|%>
          <tr>
            <td><%= users.id %></td>
            <td><%= users.name %></td>
            <td><%= users.email %></td>
            <td><%= users.created_at %></td>
            <td><%= users.updated_at %></td>
          </tr>
        <% end %>
      <% end %>
    </table>
  </div>
</div>